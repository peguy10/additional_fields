<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_sub_family_form" model="ir.ui.view">
        <field name="name">product.sub.family.form</field>
        <field name="model">product.sub.family</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code_sub_family"/>
                        <field name="name"/>
                        <field name="family_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_sub_family_tree" model="ir.ui.view">
        <field name="name">product.sub.family.tree</field>
        <field name="model">product.sub.family</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code_sub_family"/>
                <field name="name"/>
                <field name="family_id"/>
            </tree>
        </field>
    </record>
</odoo>
